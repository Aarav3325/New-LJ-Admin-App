<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.collegeapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents"
        tools:targetApi="31">
        <activity
            android:name="com.example.collegeapp.LoginActivity"
            android:theme="@style/Theme.MaterialComponents.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.collegeapp.notice.DeleteNoticeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.faculty.UpdateTeacherActivity"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.faculty.AddTeacher"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.faculty.UpdateFaculty"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.UploadPdfActivity"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.UploadImage"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.MainActivity2"
            android:exported="false" />
        <activity android:name="com.example.collegeapp.notice.UploadNotice" />
        <activity
            android:name="com.example.collegeapp.MainActivity"
            android:exported="false">

        </activity>
    </application>

</manifest>